#include <stdio.h>
#include <string.h>

int main() {
    char word[] = "yellow";
    char result[4];
    int start = 3;
    int length = 3;

    strncpy(result, word + start, length);
    result[length] = '\0';  // Не забываем нуль-терминатор

    printf("Результат: %s\n", result);

    return 0;
}
